{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Cricut.Infrastructure.Security": "Warning"
      }
    }
  },
  "DataSource": {
    "BaseUrl": "https://savelife.in.ua/wp-json/savelife",
    "EndpointTemplate": "reporting/income?date_from={DATE_FROM}&date_to={DATE_TO}&page={PAGE}&per_page={PER_PAGE}",
    "BatchSize": 50 // 100
  },
  "Loader": {
    "LoadFrom": "2023-01-01T00:00:00",
    "LoadTo": "2023-01-31T23:59:59",
    "ThrottleSeconds": 5,
    "MaxSeccondsPerOperation": 5, // 8
    "MaxIterationsCount": 240
  }

}
